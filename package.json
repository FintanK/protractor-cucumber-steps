{
  "name": "protractor-cucumber-steps",
  "version": "0.8.0",
  "description": "Cucumber steps (step definitions) written with Protractor for e2e tests",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.x"
  },
  "scripts": {
    "postinstall": "webdriver-manager update",
    "lint": "node_modules/.bin/eslint *.js tests/**/*.js",
    "test": "node_modules/.bin/protractor tests/protractor.conf.js",
    "patch": "npm version patch -m \"Bumped up package version to %s\" && git push && npm publish",
    "minor": "npm version minor -m \"Bumped up package version to %s\" && git push && npm publish",
    "major": "npm version major -m \"Bumped up package version to %s\" && git push && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Marketionist/protractor-cucumber-steps"
  },
  "keywords": [
    "protractor cucumber steps",
    "protractor-cucumber-steps",
    "cucumber steps",
    "cucumber step definitions",
    "protractor plugin",
    "test",
    "testing",
    "automated tests",
    "e2e tests",
    "end-to-end tests",
    "webdriver",
    "webdriverjs",
    "selenium",
    "protractor",
    "cucumber",
    "cucumberjs",
    "gherkin",
    "bdd"
  ],
  "author": "Dmytro Shpakovskyi",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "cucumber": "^2.3.0",
    "eslint": "^3.17.1",
    "node-testing-server": "^1.1.1",
    "protractor": "^5.1.1",
    "protractor-cucumber-framework": "^3.1.0"
  }
}
